{
	"name": "react-planet",
	"version": "1.0.1",
	"description": "A react lib for building circular menus in a very easy and handy way.",
	"author": "innFactory",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/innFactory/react-planet"
	},
	"keywords": [
		"react-planet",
		"planet",
		"planet-menu",
		"circular",
		"round",
		"circular-menu",
		"menu",
		"submenu"
	],
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"jsnext:main": "dist/index.es.js",
	"engines": {
		"node": ">=8",
		"npm": ">=5"
	},
	"scripts": {
		"test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
		"test:watch": "react-scripts-ts test --env=jsdom",
		"build": "rollup -c",
		"start": "start-storybook -p 9001 -c .storybook -s ./src/stories/public",
		"prepare": "npm run build",
		"predeploy": "npm install && npm run build",
		"deploy": "storybook-to-ghpages",
		"storybook": "npm start",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@mui/base": "^5.0.0-alpha.114",
		"@mui/system": "^5.11.5",
		"autoprefixer": "^10.4.13",
		"css-loader": "^6.7.3",
		"css-minimizer-webpack-plugin": "^4.2.2",
		"react-spring": "^8.0.27",
		"react-use-gesture": "^7.0.15",
		"rollup-plugin-postcss": "^4.0.2",
		"use-resize-observer": "^9.1.0"
	},
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.7.7",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.4.0",
		"@types/classnames": "2.2.7",
		"@types/jest": "^24.0.24",
		"@types/react": ">=18.0.0",
		"@types/react-dom": ">=18.0.0",
		"@types/react-motion": "0.0.29",
		"@types/react-virtualized": "9.18.11",
		"awesome-typescript-loader": "^5.2.1",
		"babel-loader": "^8.0.6",
		"jest": "^24.9.0",
		"prettier": "2.0.2",
		"react": "^18.2.0",
		"react-dom": ">=18.0.0",
		"rollup": ">=1.27.13",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-peer-deps-external": ">=2.2.0",
		"rollup-plugin-typescript2": ">=0.25.3",
		"sass": "^1.57.1",
		"ts-jest": "^24.2.0",
		"typescript": "3.7.5"
	},
	"files": [
		"dist"
	]
